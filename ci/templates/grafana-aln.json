{
  "title": "ALN Core Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "panels": [
    {
      "type": "graph",
      "title": "Request Rate",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{app=\"aln-core\"}[5m]))",
          "legendFormat": "req/s"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Error Rate",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{app=\"aln-core\",code=~\"5..\"}[5m]))",
          "legendFormat": "5xx req/s"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Latency p95",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_request_duration_seconds_bucket{app=\"aln-core\"}[5m])))",
          "legendFormat": "p95"
        }
      ]
    }
  ]
}
